{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "components/**/*",
    "components/**/*.vue",
    "directives/**/*",
    "utils/**/*",
    "../../storybook/stories/PinInput/PinInputDemo.vue",
    "../../storybook/stories/PinInput/PinInput.stories.ts"
  ],
  "exclude": ["components/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": "."
  },
  "vueCompilerOptions": {
    "plugins": ["unplugin-vue-macros/volar"],
    "vueMacros": {
      "exportRender": true
    }
  }
}
